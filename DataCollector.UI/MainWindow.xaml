<Window x:Class="DataCollector.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Background="#202020">
        <StackPanel Margin="50" Orientation="Vertical">
            <Button Background="DeepPink" Width="200" Margin="20" Click="CreateButtonBase_OnClick">Create schema</Button>
            <Button Background="DeepPink" Width="200" Margin="10" Click="AgendaButtonBase_OnClick">Agenda</Button>
            <Button Background="DeepPink" Width="190" Margin="20,10,10,10" Click="AgendaFilterButtonBase_OnClick">Agenda Filter</Button>
            <Button Background="DeepPink" Width="190" Margin="20,10,10,10" Click="AgendaSortButtonBase_OnClick">Agenda Sort</Button>
            <Button Background="DeepPink" Width="200" Margin="10" Click="OutletsButtonBase_OnClick">Outlets</Button>
            <Button Background="DeepPink" Width="200" Margin="10" Click="SettingsButtonBase_OnClick">Settings</Button>
        </StackPanel>
    </Grid>
</Window>
